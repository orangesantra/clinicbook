<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Timingperperson">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar11"
        android:layout_width="409dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="13dp"
        android:layout_marginEnd="1dp"
        android:background="#1081B8"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:title="Timing per person"
        app:titleTextColor="#FFFFFF" />

    <TextView
        android:id="@+id/textView68"
        android:layout_width="177dp"
        android:layout_height="36dp"
        android:layout_marginStart="19dp"
        android:layout_marginTop="69dp"
        android:background="@drawable/back"
        android:gravity="center"
        android:text="Set timing per person"
        android:textColor="#070707"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbar11" />

    <TextView
        android:id="@+id/textView77"
        android:layout_width="177dp"
        android:layout_height="50dp"
        android:layout_marginStart="19dp"
        android:layout_marginTop="119dp"
        android:background="@drawable/back"
        android:gravity="center"
        android:text="Set timing gap between  two person"
        android:textColor="#070707"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbar11" />

    <Spinner
        android:id="@+id/tspinner"
        android:layout_width="116dp"
        android:layout_height="31dp"
        android:layout_marginStart="6dp"
        android:layout_marginTop="119dp"
        android:background="@drawable/back"
        app:layout_constraintStart_toEndOf="@+id/textView68"
        app:layout_constraintTop_toBottomOf="@+id/toolbar11" />

    <Spinner
        android:id="@+id/tspinner2"
        android:layout_width="116dp"
        android:layout_height="31dp"
        android:layout_marginStart="6dp"
        android:layout_marginTop="69dp"
        android:background="@drawable/back"
        app:layout_constraintStart_toEndOf="@+id/textView68"
        app:layout_constraintTop_toBottomOf="@+id/toolbar11" />

    <TextView
        android:id="@+id/textView76"
        android:layout_width="56dp"
        android:layout_height="31dp"
        android:layout_marginStart="7dp"
        android:layout_marginTop="119dp"
        android:background="@drawable/back"
        android:gravity="center"
        android:text="MIN"
        app:layout_constraintStart_toEndOf="@+id/tspinner"
        app:layout_constraintTop_toBottomOf="@+id/toolbar11" />

    <TextView
        android:id="@+id/textView78"
        android:layout_width="56dp"
        android:layout_height="31dp"
        android:layout_marginStart="7dp"
        android:layout_marginTop="69dp"
        android:background="@drawable/back"
        android:gravity="center"
        android:text="MIN"
        app:layout_constraintStart_toEndOf="@+id/tspinner"
        app:layout_constraintTop_toBottomOf="@+id/toolbar11" />

    <Button
        android:id="@+id/button4"
        android:layout_width="84dp"
        android:layout_height="42dp"
        android:layout_marginTop="240dp"
        android:layout_marginEnd="67dp"
        android:background="#1081B8"
        android:onClick="savetime"
        android:text="Save"
        android:textColor="#FFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tspinner" />

    <Button
        android:id="@+id/button5"
        android:layout_width="85dp"
        android:layout_height="42dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="284dp"
        android:background="#1081B8"
        android:onClick="canceltiming"
        android:text="Cancel"
        android:textColor="#FFFFFF"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView68" />

    <ImageView
        android:id="@+id/imageView20"
        android:layout_width="39dp"
        android:layout_height="26dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.728"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.218"
        app:srcCompat="@drawable/ic_arrow_drop_down_black_24dp" />

    <ImageView
        android:id="@+id/imageView21"
        android:layout_width="39dp"
        android:layout_height="26dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.728"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tspinner2"
        app:layout_constraintVertical_bias="0.043"
        app:srcCompat="@drawable/ic_arrow_drop_down_black_24dp" />
</androidx.constraintlayout.widget.ConstraintLayout>