<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    tools:context=".Recyclerviewdetails.Requestlist">


    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar9"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="25dp"
        android:background="#1081B8"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:title="Clinic Info"
        app:titleTextColor="#FFFFFF" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="371dp"
        android:layout_height="32dp"
        android:layout_marginTop="168dp"
        android:background="#1081B8"
        android:clickable="true"
        android:gravity="center"
        android:onClick="clickfor"
        android:text="Show Booked Appointment List"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        android:textColor="#FFFFFF"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="@+id/guideline7"
        app:layout_constraintTop_toBottomOf="@+id/toolbar9" />

    <TextView
        android:id="@+id/textView67"
        android:layout_width="371dp"
        android:layout_height="32dp"
        android:layout_marginTop="208dp"
        android:background="#1081B8"
        android:clickable="true"
        android:gravity="center"
        android:onClick="timingperperson"
        android:text="Timing per person"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        android:textColor="#FFFFFF"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="@+id/guideline7"
        app:layout_constraintTop_toBottomOf="@+id/toolbar9" />

    <TextView
        android:id="@+id/textView35"
        android:layout_width="74dp"
        android:layout_height="33dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/back"
        android:gravity="center"
        android:text="NAME"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.062"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbar9" />

    <TextView
        android:id="@+id/textView56"
        android:layout_width="99dp"
        android:layout_height="33dp"
        android:layout_marginTop="60dp"
        android:background="@drawable/back"
        android:gravity="center"
        android:text="LOCATION"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.067"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbar9" />

    <TextView
        android:id="@+id/textView64"
        android:layout_width="371dp"
        android:layout_height="33dp"
        android:layout_marginTop="128dp"
        android:background="#1081B8"
        android:clickable="true"
        android:gravity="center"
        android:onClick="timealloted"
        android:text="Timing Alloted"
        android:textColor="#FFFFFF"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="@+id/guideline7"
        app:layout_constraintTop_toBottomOf="@+id/toolbar9" />

    <TextView
        android:id="@+id/textView63"
        android:layout_width="267dp"
        android:layout_height="33dp"
        android:layout_marginStart="96dp"
        android:layout_marginTop="60dp"
        android:background="@drawable/back"
        android:paddingLeft="10dp"
        android:text="LOCATION"
        android:textSize="12sp"
        app:layout_constraintEnd_toStartOf="@+id/guideline8"
        app:layout_constraintHorizontal_bias="0.888"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbar9" />

    <TextView
        android:id="@+id/textView49"
        android:layout_width="291dp"
        android:layout_height="33dp"
        android:layout_marginStart="90dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/back"
        android:gravity="center"
        android:text="Clinic1"
        android:textSize="16sp"
        app:layout_constraintEnd_toStartOf="@+id/guideline8"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbar9" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="18dp"
        app:layout_constraintStart_toStartOf="parent" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="390dp" />


</androidx.constraintlayout.widget.ConstraintLayout>

