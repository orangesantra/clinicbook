<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".hosclidocdetailslist.Doctormoredetails">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar16"
        android:layout_width="409dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:background="#1081B8"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:title="More Details"
        app:titleTextColor="#FFFFFF" />

    <TextView
        android:id="@+id/textView92"
        android:layout_width="130dp"
        android:layout_height="32dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="85dp"
        android:gravity="fill"
        android:text="Specialization"
        android:textColor="#101010"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbar16" />

    <TextView
        android:id="@+id/specializtion"
        android:layout_width="190dp"
        android:layout_height="32dp"
        android:layout_marginStart="160dp"
        android:layout_marginTop="85dp"
        android:background="@drawable/back"
        android:gravity="fill"
        android:paddingStart="5dp"
        android:text="____________________"
        android:textColor="#101010"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbar16" />

    <TextView
        android:id="@+id/drxyz"
        android:layout_width="330dp"
        android:layout_height="34dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="30dp"
        android:background="@drawable/back"
        android:gravity="fill"
        android:paddingLeft="10dp"
        android:text="Dr.xyz"
        android:textColor="#101010"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbar16" />

    <TextView
        android:id="@+id/textView94"
        android:layout_width="130dp"
        android:layout_height="32dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="125dp"
        android:gravity="fill"
        android:text="Experiance"
        android:textColor="#1B1919"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbar16" />

    <TextView
        android:id="@+id/experiance"
        android:layout_width="190dp"
        android:layout_height="32dp"
        android:layout_marginStart="160dp"
        android:layout_marginTop="125dp"
        android:background="@drawable/back"
        android:gravity="fill"
        android:paddingStart="5dp"
        android:text="____________________"
        android:textColor="#1B1919"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbar16" />

    <TextView
        android:id="@+id/textView99"
        android:layout_width="130dp"
        android:layout_height="32dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="205dp"
        android:gravity="fill"
        android:text="Email id"
        android:textColor="#131313"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbar16" />

    <TextView
        android:id="@+id/emailidinfo"
        android:layout_width="190dp"
        android:layout_height="32dp"
        android:layout_marginStart="160dp"
        android:layout_marginTop="205dp"
        android:background="@drawable/back"
        android:gravity="fill"
        android:paddingStart="5dp"
        android:text="____________________"
        android:textColor="#131313"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbar16" />

    <TextView
        android:id="@+id/textView100"
        android:layout_width="130dp"
        android:layout_height="32dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="165dp"
        android:gravity="fill"
        android:text="Contact no"
        android:textColor="#171616"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbar16" />

    <TextView
        android:id="@+id/contacttinfo"
        android:layout_width="190dp"
        android:layout_height="32dp"
        android:layout_marginStart="160dp"
        android:layout_marginTop="165dp"
        android:background="@drawable/back"
        android:gravity="fill"
        android:paddingStart="5dp"
        android:text="____________________"
        android:textColor="#171616"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbar16" />

    <TextView
        android:id="@+id/textView96"
        android:layout_width="157dp"
        android:layout_height="32dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="310dp"
        android:gravity="center|fill"
        android:paddingStart="7dp"
        android:text="About.~"
        android:textColor="#000000"
        android:textSize="20sp"
        android:textStyle="bold|italic"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbar16" />

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView9"
        android:layout_width="373dp"
        android:layout_height="90dp"
        app:cardCornerRadius="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.421"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.745">

        <TextView
            android:id="@+id/detailedabout"
            android:layout_width="match_parent"
            android:layout_height="90dp"
            android:background="@drawable/back"
            android:paddingStart="5dp"
            android:paddingTop="5dp"
            android:text="TextView" />
    </androidx.cardview.widget.CardView>

    <CheckBox
        android:id="@+id/bookmark"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="21dp"
        android:background="#FFFFFF"
        android:backgroundTint="#FFFFFF"
        android:onClick="bookmark"
        android:text="Bookmark"
        android:textColor="#035D88"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView99" />

    <TextView
        android:id="@+id/textView116"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="250dp"
        android:layout_marginTop="52dp"
        android:text="Available Now"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/emailidinfo" />

    <TextView
        android:id="@+id/callnowdoc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="20dp"
        android:clickable="true"
        android:onClick="callnowdoctor"
        android:text="Call"
        android:textSize="18sp"
        app:layout_constraintStart_toEndOf="@+id/callnowdoctor"
        app:layout_constraintTop_toBottomOf="@+id/emailidinfo" />

    <ImageView
        android:id="@+id/callnowdoctor"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="124dp"
        android:layout_marginTop="20dp"
        app:layout_constraintStart_toEndOf="@+id/bookmark"
        app:layout_constraintTop_toBottomOf="@+id/emailidinfo"
        app:srcCompat="@drawable/ic_baseline_call_24" />

    <RatingBar
        android:id="@+id/ratingBardoc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="152dp"
        android:layout_marginTop="60dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cardView9" />

    <TextView
        android:id="@+id/textView118"
        android:layout_width="94dp"
        android:layout_height="26dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="80dp"
        android:text="Rating"
        android:textSize="20sp"
        android:textStyle="bold|italic"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cardView9" />

    <TextView
        android:id="@+id/textView119"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="17dp"
        android:layout_marginEnd="308dp"
        android:text="Achivements"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cardView9" />

    <TextView
        android:id="@+id/award1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="58dp"
        android:layout_marginTop="17dp"
        android:text="Award 1"
        app:layout_constraintStart_toEndOf="@+id/textView119"
        app:layout_constraintTop_toBottomOf="@+id/cardView9" />

    <TextView
        android:id="@+id/award2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="132dp"
        android:layout_marginTop="17dp"
        android:text="Award 2"
        app:layout_constraintStart_toEndOf="@+id/textView119"
        app:layout_constraintTop_toBottomOf="@+id/cardView9" />

    <TextView
        android:id="@+id/award3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="204dp"
        android:layout_marginTop="17dp"
        android:text="Award 3"
        app:layout_constraintStart_toEndOf="@+id/textView119"
        app:layout_constraintTop_toBottomOf="@+id/cardView9" />

</androidx.constraintlayout.widget.ConstraintLayout>